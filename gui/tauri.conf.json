{
  "build": {
    "beforeBuildCommand": "",
    "beforeDevCommand": "",
    "devPath": "http://127.0.0.1:4173/",
    "distDir": "./gui/dist"
  },
  "package": {
    "productName": "Collector GUI",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
      "all": false
    },
    "bundle": {
      "active": true,
      "identifier": "com.endlvm.collector-gui",
      "targets": ["msi"],
      "externalBin": []
    },
    "security": {
      "csp": null,
      "dangerousRemoteDomainIpcAccess": [
        {
          "domain": "127.0.0.1",
          "windows": ["main"],
          "enable-tauri-api": true
        }
      ]
    },
    "windows": []
  }
}
